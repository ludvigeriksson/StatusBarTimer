<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>items</key>
	<array>

		<!-- ENABLE GROUP -->

		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>ENABLE_FOR</string>
		</dict>

		<!-- Timer -->
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<true/>
			<key>defaults</key>
			<string>com.ludvigeriksson.statusbartimerprefs</string>
			<key>key</key>
			<string>SBTEnableForTimer</string>
			<key>label</key>
			<string>TIMER</string>
			<key>PostNotification</key>
			<string>com.ludvigeriksson.statusbartimerprefs/settingschanged</string>
		</dict>

		<!-- Stopwatch -->
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<true/>
			<key>defaults</key>
			<string>com.ludvigeriksson.statusbartimerprefs</string>
			<key>key</key>
			<string>SBTEnableForStopwatch</string>
			<key>label</key>
			<string>STOPWATCH</string>
			<key>PostNotification</key>
			<string>com.ludvigeriksson.statusbartimerprefs/settingschanged</string>
		</dict>

		<!-- APPEARANCE GROUP -->

		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>APPEARANCE</string>
		</dict>

		<!-- Separator -->
		<dict>
			<key>cell</key>
			<string>PSEditTextCell</string>
			<key>defaults</key>
			<string>com.ludvigeriksson.statusbartimerprefs</string>
			<key>key</key>
			<string>SBTSeparator</string>
			<key>label</key>
			<string>SEPARATOR</string>
			<key>default</key>
			<string>|</string>
			<key>PostNotification</key>
			<string>com.ludvigeriksson.statusbartimerprefs/settingschanged</string>
			<key>cellClass</key>
			<string>StatusBarTimerTextCell</string>
		</dict>

		<!-- Always show minutes -->
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<false/>
			<key>defaults</key>
			<string>com.ludvigeriksson.statusbartimerprefs</string>
			<key>key</key>
			<string>SBTAlwaysShowMinutes</string>
			<key>label</key>
			<string>ALWAYS_SHOW_MINUTES</string>
			<key>PostNotification</key>
			<string>com.ludvigeriksson.statusbartimerprefs/settingschanged</string>
		</dict>

		<!-- DEVELOPER GROUP -->

		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>DEVELOPER</string>
			<key>footerText</key>
			<string>COPYRIGHT_NOTICE</string>
		</dict>

		<!-- Visit website -->
		<dict>
			<key>cell</key>
			<string>PSButtonCell</string>
			<key>label</key>
			<string>VISIT_WEBSITE</string>
			<key>action</key>
			<string>openWebsite</string>
			<key>icon</key>
			<string>WebsiteIcon.png</string>
		</dict>

		<!-- Contact -->
		<dict>
			<key>cell</key>
			<string>PSButtonCell</string>
			<key>label</key>
			<string>CONTACT</string>
			<key>action</key>
			<string>contact</string>
			<key>icon</key>
			<string>MailIcon.png</string>
		</dict>

		<!-- Donate via PayPal -->
		<dict>
			<key>cell</key>
			<string>PSButtonCell</string>
			<key>label</key>
			<string>DONATE_PAYPAL</string>
			<key>action</key>
			<string>donatePayPal</string>
			<key>icon</key>
			<string>PayPalIcon.png</string>
		</dict>

	</array>
	<key>title</key>
	<string>StatusBarTimer</string>
</dict>
</plist>
